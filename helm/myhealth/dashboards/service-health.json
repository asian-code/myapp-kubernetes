{
  "dashboard": {
    "title": "Service Health",
    "panels": [
      {
        "title": "HTTP Request Rate",
        "targets": [
          { "expr": "rate(http_requests_total[5m])" }
        ],
        "type": "graph"
      },
      {
        "title": "Error Rate",
        "targets": [
          { "expr": "rate(http_requests_total{status=~'5..'}[5m])" }
        ],
        "type": "graph"
      },
      {
        "title": "Database Query Latency",
        "targets": [
          { "expr": "db_query_duration_seconds" }
        ],
        "type": "graph"
      }
    ]
  }
}
